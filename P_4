#include <stdio.h>
#define MAX 100

int arr[MAX];
int front1 = 0, rear1 = -1;
int front2 = MAX-1, rear2 = MAX;

void enqueue1(int x) {
    if (rear1 + 1 == rear2) {
        printf("Queue Overflow\n");
        return;
    }
    arr[++rear1] = x;
}

void enqueue2(int x) {
    if (rear1 + 1 == rear2) {
        printf("Queue Overflow\n");
        return;
    }
    arr[--rear2] = x;
}

int dequeue1() {
    if (front1 > rear1) {
        printf("Queue1 Underflow\n");
        return -1;
    }
    return arr[front1++];
}

int dequeue2() {
    if (front2 < rear2) {
        printf("Queue2 Underflow\n");
        return -1;
    }
    return arr[front2--];
}

int main() {
    enqueue1(10);
    enqueue1(20);
    enqueue2(100);
    enqueue2(200);

    printf("Dequeue1: %d\n", dequeue1());
    printf("Dequeue2: %d\n", dequeue2());
    return 0;
}
